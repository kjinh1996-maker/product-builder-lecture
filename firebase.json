{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".git/**",
      ".venv/**",
      ".firebase/**",
      ".idx/**",
      ".vscode/**",
      "scripts/**",
      "GEMINI.md",
      "blueprint.md",
      "main.js",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
